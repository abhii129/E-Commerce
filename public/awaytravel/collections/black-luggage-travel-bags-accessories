  @media (prefers-reduced-motion: no-preference) {
    collage-animation img,
    collage-animation [data-collage-content] {
      opacity: 0;
    }
    @keyframes fadeInUp {
      0% {
        transform: translateY(100%);
        opacity: 0;
      }
      100% {
        transform: translateY(0%);
        opacity: 1;
      }
    }

    :root {
      --animation-delay: 100ms;
    }

    .fadeInUp-animation > * {
      animation: 1.5s fadeInUp var(--animation-delay) forwards;
    }
    collage-animation img:nth-of-type(1) {
      --animation-delay: 100ms;
    }
    collage-animation img:nth-of-type(2) {
      --animation-delay: 150ms;
    }
    collage-animation img:nth-of-type(3) {
      --animation-delay: 200ms;
    }
    collage-animation img:nth-of-type(4) {
      --animation-delay: 250ms;
    }
    collage-animation img:nth-of-type(5) {
      --animation-delay: 300ms;
    }
    collage-animation img:nth-of-type(6) {
      --animation-delay: 350ms;
    }
  }
</style>

<!-- ##entry##collage-animation##endentry## -->

<collage-animation class='h-full max-w-[100vw] py-lg min-h-[630px] aspect-[0.56] md:aspect-[1.8] relative flex justify-center items-center 2xl:container'>
  <div
    data-collage-content
    class='flex flex-col gap-sm justify-center items-center px-sm text-center md:max-w-[46%]'
    
      id='collage'
    
    
  >
    
