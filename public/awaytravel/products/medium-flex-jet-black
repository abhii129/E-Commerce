        style_sheet: '//www.awaytravel.com/cdn/shop/t/771/assets/-powerreviews.css?v=106080686077395897521754414660',
        components: {
          ReviewDisplay: 'pr-reviewdisplay',
          ReviewSnippet: 'pr-reviewsnippet',
        },
        on_render: (config, data) => {
          if (data.review_count !== undefined && data.review_count > 0) {
            return
          }
          if (config.component === "ReviewSnippet") {
            const noReviewsElements = document.querySelectorAll(
              ".p-w-r .pr-no-reviews .pr-snippet-review-count"
            )
            noReviewsElements.forEach((noReviewsElement) => {
              noReviewsElement.innerHTML = "No reviews yet"
            })
          }
          if (config.component === "ReviewDisplay") {
            const reviewDisplayWrapperElements = document.querySelectorAll(
              ".pr-reviewdisplay-container"
            )
            reviewDisplayWrapperElements.forEach((reviewDisplayWrapper) => {
              reviewDisplayWrapper.setAttribute("data-no-reviews", "true")
            })
          }
        },
      })
      setTimeout(() => {
        document.querySelector('#pr-reviewsnippet')?.classList.remove('opacity-0')
      }, 100)
  </script>


    
