            class="bg-t-brand-secondary border-0 pt-6 pr-md pb-0 body-small block w-full border appearance-none text-inherit disabled:text-t-border border-t-0 border-l-0 border-r-0 bg-t-background border-t-brand-primary disabled:border-t-border focus:outline-none focus:ring-0 focus:border-t-foreground peer group-[[error]]:ring-u-error group-[[error]]:border-[transparent] group-[[error]]:ring-2 py-sm mb-xs rounded-none"
            placeholder=" "
            type="email"
            id="email"
            name="contact[email]"
            
              value=""
            

            

            

            

            
              required
            

            

            

            

            
          />

          
